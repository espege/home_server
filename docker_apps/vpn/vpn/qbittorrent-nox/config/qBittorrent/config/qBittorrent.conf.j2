[Application]
FileLogger\Age=1
FileLogger\AgeType=1
FileLogger\Backup=true
FileLogger\DeleteOld=true
FileLogger\Enabled=true
FileLogger\MaxSizeBytes=66560
FileLogger\Path=/config/qBittorrent/data/logs

[BitTorrent]
Session\AddExtensionToIncompleteFiles=true
Session\AddTorrentStopped=false
Session\DefaultSavePath=/downloads
Session\DisableAutoTMMByDefault=false
Session\ExcludedFileNames=
Session\FinishedTorrentExportDirectory=/completed_torrents
Session\GlobalMaxRatio=10
Session\Interface=tun0
Session\InterfaceName=tun0
Session\MaxActiveDownloads=4
Session\MaxActiveUploads=4
Session\Port=6881
Session\Preallocation=true
Session\QueueingSystemEnabled=true
Session\SSL\Port=20722
Session\ShareLimitAction=Stop
Session\TempPath=/downloads
Session\TempPathEnabled=true
Session\TorrentExportDirectory=/incomplete_torrents

[Core]
AutoDeleteAddedTorrentFile=Never

[LegalNotice]
Accepted={{ (vpn_config.QBT_EULA == 'confirm') | ternary('true', 'false') | default('true') }}

[Meta]
MigrationVersion=8

[Network]
Cookies=@Invalid()
Proxy\HostnameLookupEnabled=false
Proxy\Profiles\BitTorrent=true
Proxy\Profiles\Misc=true
Proxy\Profiles\RSS=true

[Preferences]
General\Locale=en
MailNotification\req_auth=true
WebUI\AuthSubnetWhitelist={{ ([local_subnet_range] + [vpn_config.QBT_NETWORK_IP_RANGE]) | join(', ') }}
WebUI\AuthSubnetWhitelistEnabled=true
WebUI\HostHeaderValidation=false
WebUI\LocalHostAuth=false
WebUI\Password_PBKDF2="{{ vpn_config.QBT_WEBUI_PASSWORD_2}}"
WebUI\Username={{ vpn_config.QBT_WEBUI_USERNAME }}

[RSS]
AutoDownloader\DownloadRepacks=true
AutoDownloader\SmartEpisodeFilter=s(\\d+)e(\\d+), (\\d+)x(\\d+), "(\\d{4}[.\\-]\\d{1,2}[.\\-]\\d{1,2})", "(\\d{1,2}[.\\-]\\d{1,2}[.\\-]\\d{4})"
Session\EnableProcessing=false
Session\RefreshInterval=600
